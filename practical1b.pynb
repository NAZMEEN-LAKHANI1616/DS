#addition
import numpy as np

M1 = np.array([[6, 7, 9], [6, -10, 15], [-7, 14, 21]])
M2 = np.array([[12, -11, 18], [11, 22, 33], [13, -26, 39]])
M3 = M1 + M2  
print(M3)

#multiplication
import numpy as np

M1 = np.array([[18, 21], [25, -10]])
M2 = np.array([[12, -18], [21, 22]])
M3 = M1.dot(M2)  
print(M3)

#transpose
import numpy as np

M1 = np.array([[31, 16, 19], [15, -10, 25], [41,18,12]])
M2 = M1.transpose()

print(M2)
